{
  "compilerOptions": {
    "rootDir": ".",
    "target": "es5",
    "lib": [
      "es2017"
    ],
    "downlevelIteration": true,
    "sourceMap": true,
    "watch": false,
    "strictNullChecks": true,
    "alwaysStrict": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "typeRoots": [
      "node_modules/augur-types/types",
      "node_modules/@types"
    ]
  }
}

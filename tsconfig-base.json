{
  "compilerOptions": {
    "composite": true,
    "target": "es5",
    "lib": [
      "es2015",
      "dom",
      "webworker",
      "es2017"
    ],
    "declarationMap": true,
    "downlevelIteration": true,
    "inlineSources": true,
    "inlineSourceMap": true,
    "watch": false,
    "strictNullChecks": true,
    "alwaysStrict": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "typeRoots": [
      "node_modules/@augurproject/types/types",
      "node_modules/@types"
    ]
  }
}

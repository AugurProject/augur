{
  "presets":     [
    "@babel/preset-react",
    "@babel/preset-env"
  ],
  "plugins": [
    "transform-class-properties",
    "@babel/plugin-syntax-dynamic-import",
    "@babel/plugin-transform-runtime"
  ],
  "env": {
    "production": {
      "plugins": [
        [
          "transform-react-remove-prop-types",
          {
            "ignoreFilenames": ["node_modules"],
            "mode": "remove",
            "removeImport": true
          }
        ],
      ],
      "compact": false,
    },
  }
}

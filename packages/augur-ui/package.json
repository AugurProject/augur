{
  "name": "@augurproject/ui",
  "version": "2.0.0-alpha.2",
  "description": "Augur UI",
  "author": "The Augur Developers <team@augur.net>",
  "license": "AAL",
  "engines": {
    "node": ">= 8"
  },
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/AugurProject/augur-ui"
  },
  "scripts": {
    "clean": "rm -rf build; rm -rf node_modules",
    "nuke": "yarn clean; rm yarn.lock; rm package-lock.json",
    "dev": "yarn dev:orbit && yarn dev:ui",
    "dev:orbit": "yarn workspace orbit-web build",
    "dev:ui": "node scripts/dev.js",
    "build": "yarn workspace orbit-web build && webpack",
    "build:kovan": "ETHEREUM_NETWORK=kovan yarn workspace orbit-web build && ETHEREUM_NETWORK=kovan webpack",
    "build:mainnet": "ETHEREUM_NETWORK=mainnet yarn workspace orbit-web build && ETHEREUM_NETWORK=mainnet webpack",
    "start": "node server.js",
    "__": "",
    "integration:headless": "AUGUR_URL='http://localhost:8080/' jest --runInBand --detectOpenHandles --forceExit",
    "integration": "HEADLESS=false npm run integration:headless",
    "lint": "node scripts/lint.js",
    "lit": "echo 'so lit'",
    "eslint-check": "eslint --print-config . | eslint-config-prettier-check",
    "format": "prettier --write '**/*.{js,jsx,ts,tsx}'",
    "test": "jest",
    "kickoff": "./scripts/setup-local-dev.sh",
    "heroku-postbuild": "webpack  --debug --config config/webpack.heroku.config.js",
    "reinstall": "./scripts/reinstall.sh",
    "regenerate-lockfiles": "rm -f package-lock.json && npm install && rm -f yarn.lock && yarn",
    "docker:build": "docker build --tag augurproject/augur-ui:latest  .",
    "docker:push": "docker push augurproject/augur-ui:latest",
    "docker:release:stable": "scripts/release-stable.sh",
    "docker:release": "scripts/release-docker.sh",
    "docker:spin-up": "npm run docker:pull && npm run docker:start",
    "git:push:protected": "PUSH_PROTECTED_BRANCHES=true git push",
    "prepush": "yarn test && node scripts/pre-push.js"
  },
  "dependencies": {

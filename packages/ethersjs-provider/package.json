{
  "name": "@augurproject/ethersjs-provider",
  "version": "2.1.9",
  "description": "Ethers.JS Provider Implementation for Augur API",
  "author": "The Augur Developers <team@augur.net>",
  "license": "MIT",
  "main": "build",
  "directories": {
    "src": "src"
  },
  "scripts": {
    "rebuild": "yarn build",
    "lint": "tslint -c tslint.json -p . ./src/*.ts ./src/**/*.ts ./src/**/**/*.ts ./src/**/**/**/*.ts",
    "build": "tsc -b",
    "watch": "tsc -w",
    "nuke": "rm -f package-lock.json; rm -f yarn.lock; rm -rf node_modules; yarn clean",
    "prepare": "tsc -b",
    "prepush": "yarn lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AugurProject/augur.git"
  },
  "bugs": {
    "url": "https://github.com/AugurProject/augur/issues"
  },
  "homepage": "https://github.com/AugurProject/augur#readme",
  "dependencies": {
    "@augurproject/artifacts": "^2.1.9",
    "@augurproject/contract-dependencies-ethers": "^2.1.9",
    "@augurproject/types": "^2.1.1",
    "ethereum-types": "2.1.6",
    "ethers": "5.0.17"
  },
  "devDependencies": {
    "ts-node": "9.0.0",
    "typescript": "3.9.7"
  },
  "buildDependencies": [
    "typescript"
  ],
  "publishConfig": {
    "access": "public"
  }
}

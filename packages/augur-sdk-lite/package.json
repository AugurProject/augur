  "main": "build",
  "directories": {
    "src": "src"
  },
  "scripts": {
    "prepare:abi:ParaShareToken": "node ../augur-tools/build/flash/cli.js abi -c ParaShareToken > src/abi/ParaShareToken.json",
    "prepare:abi": "yarn prepare:abi:ParaShareToken",
    "build": "tsc -b",

  "dependencies": {
    "@augurproject/artifacts": "^2.1.9",
    "@augurproject/utils": "^2.1.9",
    "async": "3.1.0",
    "ethers": "5.0.17",
    "lz-string": "1.4.4"
  },
  "devDependencies": {
    "@types/async": "3.0.1",
    "@types/cors": "2.8.6",
    "@types/node": "10.14.18",
    "@types/node-fetch": "2.5.1",
    "@types/web3": "1.0.19",
    "ts-node": "9.0.0",
    "typedoc": "0.17.4",

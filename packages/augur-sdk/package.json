{
  "name": "@augurproject/sdk",
  "version": "2.0.0-alpha.2",
  "description": "Augur SDK",
  "author": "The Augur Developers <team@augur.net>",
  "license": "MIT",
  "main": "build",
  "directories": {
    "src": "src"
  },
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register ./src/state/Server",
    "connect": "ts-node -r tsconfig-paths/register ./src/connector/ws-connector.test",
    "lint": "tslint -c tslint.json -p . ./src/*.ts ./src/**/*.ts ./src/**/**/*.ts ./src/**/**/**/*.ts",
    "test": "jest",
    "build": "tsc -b",
    "watch": "tsc -b -w",
    "nuke": "rm -f package-lock.json; rm -f yarn.lock; rm -rf node_modules; yarn clean",
    "prepush": "yarn lint",
    "clean": "echo delete database",
    "rebuild": "yarn clean && yarn build",
    "clean-start": "yarn rebuild && yarn start",
    "clean-start-local": "yarn rebuild && ETHEREUM_HTTP=http://127.0.0.1:8545 ETHEREUM_WS=ws://127.0.0.1:8546 yarn start",
    "start-kovan": "ETHEREUM_HTTP=https://eth-kovan.alchemyapi.io/v2/1FomA6seLdWDvpIRvL9J5NhwPHLIGbWA yarn start",
    "build:documentation": "yarn typedoc --theme markdown --disableOutputCheck --ignoreCompilerErrors --out ../../docs/sdk ./src",
    "check": "gts check",
    "fix": "gts fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AugurProject/augur.git"
  },
  "bugs": {
    "url": "https://github.com/AugurProject/augur/issues"
  },
  "homepage": "https://github.com/AugurProject/augur#readme",
  "dependencies": {
    "@0x/mesh-browser-lite": "9.3.0",
    "@0x/mesh-rpc-client": "9.3.0",
    "@0x/order-utils": "8.4.0",
    "abi-decoder": "2.3.0",
    "@augurproject/artifacts": "2.0.0-alpha.2",
    "@augurproject/contract-dependencies-ethers": "2.0.0-alpha.2",
    "@augurproject/core": "2.0.0-alpha.2",
    "@augurproject/indexeddbshim": "6.1.1",
    "@augurproject/utils": "2.0.0-alpha.2",
    "async": "3.1.0",
    "bs58": "4.0.1",
    "comlink": "^4.3.0",
    "cors": "2.8.5",
    "cross-fetch": "3.0.4",
    "dexie": "3.0.0-rc.4",
    "ethereum-types": "2.1.6",
    "ethereumjs-blockstream": "7.0.0",
    "ethereumjs-util": "6.1.0",
    "ethereumjs-tx": "2.1.2",
    "ethers": "4.0.37",
    "express": "4.17.1",
    "flexsearch": "0.6.32",
    "io-ts": "1.10.4",
    "ipfs": "0.40.0",
    "ipld-dag-pb": "0.18.1",
    "isomorphic-ws": "4.0.1",
    "lodash": "4.17.15",
    "lz-string": "1.4.4",
    "moment": "2.24.0",
    "websocket-as-promised": "1.0.1",
    "ws": "7.1.2"
  },
  "devDependencies": {
    "@0x/mesh-browser": "9.3.0",
    "@augurproject/types": "2.0.0-alpha.2",
    "@types/async": "3.0.1",
    "@types/cors": "2.8.6",
    "@types/dexie": "1.3.1",
    "@types/express": "4.17.1",
    "@types/helmet": "0.0.44",

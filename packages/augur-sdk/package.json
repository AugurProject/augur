    "typedoc": "0.17.4",
    "typedoc-plugin-markdown": "2.2.17",
    "yarn": "1.22.0"
  },
  "buildDependencies": [
    "typescript"
  ],
  "resolutions": {
    "**/sha3": "2.0.1"
  }

{
  "name": "@augurproject/test",
  "version": "2.0.0-alpha.1",
  "description": "Augur Test",
  "author": "The Augur Developers <team@augur.net>",
  "license": "MIT",
  "main": "build",
  "directories": {
    "tests": "src/tests"
  },
  "scripts": {
    "lint": "tslint -c tslint.json -p . src/tests/*.ts ./tests/**/*.ts ./tests/**/**/*.ts ./tests/**/**/**/*.ts",
    "build": "tsc -b",
    "watch": "tsc -w",
    "nuke": "rm -f package-lock.json; rm -f yarn.lock; rm -rf node_modules; yarn clean",
    "prepare": "tsc -b",
    "clean": "echo delete database",
    "rebuild": "yarn clean && yarn build",
    "prepush": "yarn lint",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AugurProject/augur.git"
  },
  "bugs": {
    "url": "https://github.com/AugurProject/augur/issues"
  },
  "homepage": "https://github.com/AugurProject/augur#readme",
  "dependencies": {
    "@augurproject/artifacts": "2.0.0-alpha.1",
    "@augurproject/core": "2.0.0-alpha.1",
    "@augurproject/ethersjs-provider": "2.0.0-alpha.1",
    "@augurproject/sdk": "2.0.0-alpha.1",
    "@augurproject/tools": "2.0.0-alpha.1",
    "contract-dependencies-ethers": "2.0.0-alpha.1",
    "ethers": "4.0.37",
    "levelup": "4.2.0",
    "memdown": "4.1.0",
    "ethereumjs-util": "6.1.0"
  },
  "devDependencies": {
    "@types/argparse": "1.0.36",
    "@types/jest": "24.0.18",
    "@types/jest-environment-puppeteer": "4.3.1",
    "@types/node": "10.14.18",
    "@types/supertest": "2.0.8",
    "ganache-core": "2.8.0",
    "jest": "24.9.0",
    "jest-puppeteer": "4.3.0",
    "pouchdb-adapter-memory": "7.1.1",
    "puppeteer": "1.20.0",
    "supertest": "4.0.2",
    "ts-jest": "24.0.2",
    "ts-node": "8.4.1",
    "tsconfig-paths": "3.9.0"
  },
  "resolutions": {
    "**/sha3": "2.0.1",
    "**/memdown": "4.1.0"
  }
}
